<%- include("header"); -%>
    <div class="box" id="heading">
    <h1><%= listTitle %></h1>
    </div>
    <div class="box">
        <% for(var i=0;i<itemslist.length;i++){   %>
            <div class="item">
                <input type="checkbox">
            <p><%= itemslist[i] %></p>
            </div>
       <% } %>

      <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        <button type="submit" class="button_1" name="button" value=<%= listTitle %>>+</button>
    </form>

    <form action="/allNew" method="get">
        <button type="submit" class="button_2">New</button>
    </form>

</div>
<%- include("footer"); -%>
